CREATE TABLE player (
    Player_Id integer,
    Player_Name text,
    DOB date,
    Batting_Hand text,
    Bowling_Skill text,
    Country text,
    Is_Umpire text,
    faltu_delete text,
    PRIMARY KEY(Player_Id)
);



CREATE TABLE season (
    Season_Id integer,
    Season_Year integer,
    Orange_Cap_Id integer,
    Purple_Cap_Id integer,
    Man_of_the_Series_Id integer,
    PRIMARY KEY(Season_Id)
);



CREATE TABLE team (
    Team_Id integer,
    Team_Name varchar(30),
    Team_Short_Code varchar(30),
    PRIMARY KEY(Team_Id)
);



CREATE TABLE match (
    Match_Id integer,
    Match_Date date,
    Team_Name_Id integer,
    Opponent_Team_Id integer,
    Season_Id integer,
    Venue_Name varchar(30),
    Toss_Winner_Id integer,
    Toss_Decision varchar(30),
    IS_Superover integer,
    IS_Result integer,
    Is_DuckWorthLewis integer,
    Win_Type varchar(30),
    Won_By integer,
    Match_Winner_Id integer,
    Man_Of_The_Match_Id integer,
    First_Umpire_Id integer,
    Second_Umpire_Id integer,
    City_Name varchar(30),
    Host_Country varchar(30),
    PRIMARY KEY(Match_Id)
);



CREATE TABLE player_match (
    Match_Id integer,
    Player_Id integer,
    Team_Id integer,
    Is_Keeper integer,
    Is_Captain integer,
    PRIMARY KEY(Match_Id)
);















